<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="25" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.projet_java.HelloController" >

    <padding>
        <Insets bottom="25" left="25" right="25" top="25"/>
    </padding>

    <Label fx:id="nom"/>
    <Button text="JOUER" onAction="#onPlayButtonClick"/>


    <Label fx:id="nombreJoueur"/>
    <Button text="AJOUTER" onAction="#onAddButtonClick"/>
    <Button text="RETIRER" onAction="#onDecreaseButtonClick"/>
</VBox>
